<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="semana3DA.Page1">
    <ContentPage.Content>
        <StackLayout Style="{StaticResource stackLayoutStyle}">
            
            
            
            <Label Text="SEMANA 3 :ESTILO 1" FontSize="18" HorizontalTextAlignment="Center"/>
            <Entry x:Name="txtDatoUno" Placeholder="Ingrese una cadena"> </Entry>
            <Entry x:Name="txtDatoUDos" Placeholder="Ingrese un numero" Keyboard="Numeric"></Entry>
            <Button x:Name="btnAbrir" Text="ABRIR" Clicked="btnAbrir_Clicked"> </Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>